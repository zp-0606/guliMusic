<!--pages/video/video.wxml-->
<view class="videoContainer">
<!--    头部区域-->
    <view class="header">
        <image src="/static/images/video/video.jpg"></image>
        <view class="search">搜索商品</view>
        <image src="/static/images/logo.png"></image>
    </view>
<!--    导航区域-->
    <scroll-view class="navScroll" scroll-x enable-flex>
        <view class="navItem" wx:for="{{videoGroupList}}" wx:key="id">
            <view class="navContent {{item.id==navId?'active':''}}" id="{{item.id}}" bindtap="changeNav">{{item.name}}</view>
        </view>
    </scroll-view>
</view>
